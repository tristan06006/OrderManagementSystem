
@model IEnumerable<OMS.Models.District>

    @{
        ViewData["Title"] = "List of District";
    }

    <h2>List of District</h2>

    <p>
        <a asp-action="Create">Add New</a>
    </p>
    <table class="table">
        <thead>
            <tr>
                <th>Warehouse </th>
                <th>Name </th>
                <th>Street </th>
                <th>City </th>
                <th>State</th>
                <th>ZIP</th>
                <th>Sales TAX</th>
                <th>Year to Date Balance</th>
                @*<th>Next Avail. Order</th>*@
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.D_W_NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.D_NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.D_STREET_1)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.D_CITY)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.D_STATE)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.D_ZIP)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.D_TAX)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.D_YTD)
                    </td>
                    @*<td>
                        @Html.DisplayFor(modelItem => item.D_NEXT_O_ID)
                    </td>*@
                    <td>
                        <a asp-action="Edit" asp-route-id="@item._id">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item._id">Details</a>
                        @*<a asp-action="Delete" asp-route-id="@item._id">Delete</a>*@
                    </td>
                </tr>
            }
        </tbody>
    </table>


